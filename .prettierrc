  "parser": "typescript",
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "jsx",
    "typescript",
    "importOrderParserPlugins",
    "classProperties",
    "decorators-legacy"
  ]
}